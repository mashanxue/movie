<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.movie.mapper.AddChangCiMapper">
    <insert id="addChangCi" parameterType="com.movie.entity.Changci" useGeneratedKeys="true" keyProperty="changciId" >
         insert into changci (changciName,changtingId,moviesId,time,zwId) values(#{changciName},#{changtingId},#{moviesId},#{time},#{zwId})
    </insert>
</mapper>
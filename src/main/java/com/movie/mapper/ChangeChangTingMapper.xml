<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.movie.mapper.ChangeChangTingMapper">
    <update id="changeChangTing" parameterType="com.movie.entity.Changting">
        update changting set changTingName=#{changTingName},changTingDX=#{changTingDX},yuanId=#{yuanId} where changTingId=#{changTingId}
    </update>
</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.movie.mapper.ChangeChangCiMapper">
    <update id="changeChangCi" parameterType="com.movie.entity.Changci">
        update changci set changciName=#{changciName},changtingId=#{changtingId},moviesId=#{moviesId},time=#{time},zwId=#{zwId} where changciId=#{changciId}
    </update>
</mapper>